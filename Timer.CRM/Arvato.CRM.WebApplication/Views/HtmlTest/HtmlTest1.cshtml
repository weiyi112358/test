@{
    ViewBag.Title = "HtmlTest1";
}

<div class="row-fluid heading">
    <div class="pull-left">
        <h3>积分转储值规则</h3>
    </div>
</div>

<div class="form-inline  well">
    <div class="q_con">
        <label class="ml10">兑换规则单号</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">兑换规则名称</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">积分规则单状态</label>
        <select class="span2 sepV_b" id=""></select>
    </div>
    <div class="q_con">
        <label class="ml10">兑换所需积分数</label>
        <input type="text" class="input-medium length_b" id="">
        <em class="to">-</em>
        <input type="text" class="input-medium length_b" id="">
    </div>
    <div class="q_con">
        <label class="ml10">兑换金额</label>
        <input type="text" class="input-medium length_b" id="">
        <em class="to">-</em>
        <input type="text" class="input-medium length_b" id="">
    </div>
    <div class="q_con">
        <label class="ml10">最后修改时间</label>
        <input type="text" class="input-medium length_b" placeholder='选择日期' id="">
        <em class="to">-</em>
        <input type="text" class="input-medium length_b" placeholder='选择日期' id="">
    </div>
    <div class="q_con">
        <label class="ml10">最后修改人代码</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">排序字段</label>
        <select class="span2 sepV_b" id=""></select>
    </div>
    <div class="q_con">
        <label class="ml10">排序方式</label>
        <select class="span2 sepV_b" id=""></select>
    </div>
    <div class="q_btn">
        <button class="btn" id="">查询</button>
        <button class="btn" id="">清空</button>
        <button class="btn" onclick="goEdit()">新建</button>
    </div>
</div>


<div class="row-fluid">
    <div class="mt10 mbox">
        <div id="dt_dataGroup_wrapper" class="dataTables_wrapper form-inline no-footer">
            <div class="dt-top-row"></div>
            <div id="dt_dataGroup_processing" class="dataTables_processing" style="display: none;">数据加载中……</div>
            <div class="dt-wrapper">
                <table class="table dataTable no-footer" id="dt_dataGroup" role="grid" aria-describedby="dt_dataGroup_info">
                    <thead>
                        <tr role="row">
                            <th class="sorting_asc" rowspan="1" colspan="1" style="width: 154px;" aria-label="兑换规则单号">兑换规则单号</th>
                            <th class="sorting_disabled" rowspan="1" colspan="1" style="width: 154px;" aria-label="兑换规则名称">兑换规则名称</th>
                            <th class="sorting" tabindex="0" aria-controls="dt_dataGroup" rowspan="1" colspan="1" style="width: 144px;" aria-label="兑换规则单状态">兑换规则单状态</th>
                            <th class="sorting_disabled" rowspan="1" colspan="1" style="width: 187px;" aria-label="兑换所需积分">兑换所需积分</th>
                            <th class="sorting" tabindex="0" aria-controls="dt_dataGroup" rowspan="1" colspan="1" style="width: 144px;" aria-label="兑换金额">兑换金额</th>
                            <th class="sorting" tabindex="0" aria-controls="dt_dataGroup" rowspan="1" colspan="1" style="width: 183px;" aria-label="最后修改信息">最后修改信息</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr role="row" class="odd">
                            <td class="sorting_1">123</td>
                            <td>亚夏</td>
                            <td>1</td>
                            <td>2000</td>
                            <td>2000</td>
                            <td>2014-12-30 11:27:19</td>
                        </tr>
                        <tr role="row" class="even">
                            <td class="sorting_1">234</td>
                            <td>Root</td>
                            <td>0</td>
                            <td>3000</td>
                            <td>3000</td>
                            <td>2015-02-04 09:52:26</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="dt-row dt-bottom-row mt10">
                <div class="pull-left">
                    <div class="dataTables_info" id="dt_dataGroup_info" role="status" aria-live="polite">从 1 到 2 条，共 2 条记录</div>
                </div>
                <div class="pull-right">
                    <div class="dataTables_paginate paging_bootstrap_alt pagination" id="dt_dataGroup_paginate">
                        <ul>
                            <li class="first disabled"><a href="#">&lt;&lt; 首页</a></li>
                            <li class="prev disabled"><a href="#">&lt;  上一页 </a></li>
                            <li class="active"><a href="#">1</a></li>
                            <li class="next disabled"><a href="#">下一页  &gt;</a></li>
                            <li class="last disabled"><a href="#">末页  &gt;&gt;</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

