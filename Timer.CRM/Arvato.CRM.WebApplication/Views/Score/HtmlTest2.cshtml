@{
    ViewBag.Title = "HtmlTest2";
}

<div class="row-fluid heading">
    <div class="pull-left">
        <h3>新增积分转储值规则单</h3>
    </div>
    <div class="pull-right">
        <button class="btn" type="button" onclick="">保存</button>
        <button class="btn" type="button" onclick="">重填</button>
        <button class="btn" type="button" onclick="">保存并审核</button>
        <button class="btn" type="button" onclick="">后退</button>
    </div>
</div>

<div class="form-inline well">
    <div class="title_big">新增升级规则详细信息</div>
    <div class="q_con">
        <label class="ml10">兑换规则账号</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">兑换金额<span class="f_req">*</span></label>
        <input type="text" class="input-medium" id="">
        <span class="error-block"></span>
    </div>
    <div class="q_con">
        <label class="ml10">兑换规则名称<span class="f_req">*</span></label>
        <input type="text" class="input-medium" id="">
        <span class="error-block"></span>
    </div>
    <div class="q_con">
        <label class="ml10">创建信息</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">兑换规则单状态</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">最后修改信息</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">兑换所需积分数</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">审核信息</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">作废信息</label>
        <input type="text" class="input-medium" id="">
    </div>
    <div class="q_con">
        <label class="ml10">说明</label>
        <textarea class="input-medium"></textarea>
    </div>
</div>

<div class="form-inline well disblock">
    <div class="title_big">编辑兑换规则明细列表</div>
    <div class="dhgzmx">
        <div class="operation">
            <a class="onaline puba"></a>
            <a class="nextline puba"></a>
            <a class="addline puba"></a>
        </div>
        <div class="q_con">
            <label class="ml10">行号</label>
            <input type="text" class="input-medium" id="">
        </div>
        <div class="q_con">
            <label class="ml10">积分类别</label>
            <input type="text" class="input-medium jflbh" id="">
            <input type="text" class="input-medium jflb" id="">
            <a class="choose_jf"></a>
        </div>
        <div class="q_con">
            <label class="ml10">积分数<span class="f_req">*</span></label>
            <input type="text" class="input-medium" id="">
        </div>
        <button id="" class="btn addf_btn">添加行</button>
    </div>

    <div class="dhgzmx_fr">
        <div class="operation">
            <a class="xymb puba"></a>
            <a class="plsc puba"><span>批量删除</span></a>
        </div>

        <div class="row-fluid">
            <div class="mt10 mbox">
                <div id="dt_dataGroup_wrapper" class="dataTables_wrapper form-inline no-footer">
                    <div class="dt-top-row"></div>
                    <div id="dt_dataGroup_processing" class="dataTables_processing" style="display: none;">数据加载中……</div>
                    <div class="dt-wrapper">
                        <table class="table dataTable no-footer" id="dt_dataGroup" role="grid" aria-describedby="dt_dataGroup_info">
                            <thead>
                                <tr role="row">
                                    <th class="sorting_disabled" rowspan="1" colspan="1" style="width: 10%;"><input class="input_c" type="checkbox" value="" /></th>
                                    <th class="sorting_disabled" rowspan="1" colspan="1" style="width: 30%;" aria-label="行号">行号</th>
                                    <th class="sorting_disabled" tabindex="0" aria-controls="dt_dataGroup" rowspan="1" colspan="1" style="width: 30%;" aria-label="积分类别">积分类别</th>
                                    <th class="sorting_disabled" rowspan="1" colspan="1" style="width: 30%;" aria-label="兑换所需积分">兑换所需积分</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr role="row" class="odd">
                                    <td class="sorting_1">
                                        <input class="input_c" type="checkbox" value="" /></td>
                                    <td>亚夏</td>
                                    <td>1</td>
                                    <td>2000</td>
                                </tr>
                                <tr role="row" class="even">
                                    <td class="sorting_1">
                                        <input class="input_c" type="checkbox" value="" /></td>
                                    <td>Root</td>
                                    <td>0</td>
                                    <td>3000</td>
                                </tr>
                                <tr role="row" class="even">
                                    <td class="sorting_1">234</td>
                                    <td>Root</td>
                                    <td>0</td>
                                    <td>3000</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="dt-row dt-bottom-row mt10">
                        <div class="pull-left">
                            <div class="dataTables_info" id="dt_dataGroup_info" role="status" aria-live="polite">从 1 到 2 条，共 2 条记录</div>
                        </div>
                        <div class="pull-right">
                            <div class="dataTables_paginate paging_bootstrap_alt pagination" id="dt_dataGroup_paginate">
                                <ul>
                                    <li class="first disabled"><a href="#">&lt;&lt; 首页</a></li>
                                    <li class="prev disabled"><a href="#">&lt;  上一页 </a></li>
                                    <li class="active"><a href="#">1</a></li>
                                    <li class="next disabled"><a href="#">下一页  &gt;</a></li>
                                    <li class="last disabled"><a href="#">末页  &gt;&gt;</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</div>


<!--新增弹层_选择积分类别-->
<div id="colorbox" class="hide" style=" visibility: visible; top: 0px; left: 320px; position: absolute; width: 1016px; height: 568px; overflow: hidden;">
    <div id="cboxWrapper" style="height: 568px; width: 716px;">
        <div style="clear: left;">
            <div id="cboxMiddleLeft" style="float: left; height: 536px;"></div>
            <div id="cboxContent" style="float: left; width: 716px; height: 536px;">
                <div id="cboxLoadedContent" style="width: 700px; overflow: auto; height: 520px; background-color: rgb(255, 255, 255); display: block;">
                    <div id="pageRole_dialog" class="cbox_content" style="width: 660px; height: 480px">
                        <form action="/Auth/Roles" data-ajax="true" id="frmPageRole" method="post" novalidate="novalidate">
                            <div class="row-fluid heading">
                                <div class="pull-left">
                                    <h3 id="titlePageRole">选择积分类别</h3>
                                </div>
                            </div>
                            <div class="row-fluid sepH_c" style="margin: 0px !important;">
                                <div class="span3">
                                    <label>代码 起始于</label>
                                    <input class="span12" id="" type="text">
                                </div>
                                <div class="span3">
                                    <label>名称 类似于</label>
                                    <input class="span12" id="" type="text">
                                </div>
                            </div>

                            <div class="row-fluid">
                                <div class="pull-right">
                                    <button id="" type="button" class="btn" onclick="$.colorbox.close();">取消</button>&nbsp;
                                    <button id="" type="submit" class="btn btn-info sepV_c">确认</button>&nbsp;
                                </div>
                            </div>
                            <br />
                            <div id="dt_elements_wrapper" class="dataTables_wrapper form-inline no-footer">
                                <div class="dt-top-row"></div>
                                <div id="" class="dataTables_processing" style="display: none;">数据加载中……</div>
                                <div class="dt-wrapper">
                                    <table class="table dataTable no-footer" id="" role="grid" aria-describedby="dt_elements_info">
                                        <thead>
                                            <tr role="row">
                                                <th class="sorting_disabled" style="width: 10%;"></th>
                                                <th class="sorting_disabled" rowspan="1" colspan="1" style="width: 45%;" aria-label="代码">代码</th>
                                                <th class="sorting_disabled" rowspan="1" colspan="1" style="width: 45%;" aria-label="名称">名称</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="odd">
                                                <td colspan="3" class="dataTables_empty" valign="top">没有记录</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="dt-row dt-bottom-row mt10">
                                    <div class="pull-left">
                                        <div class="dataTables_info" id="dt_elements_info" role="status" aria-live="polite">暂无记录</div>
                                    </div>
                                    <div class="pull-right">
                                        <div class="dataTables_paginate paging_bootstrap_alt pagination" id="dt_elements_paginate">
                                            <ul>
                                                <li class="first disabled"><a href="#">&lt;&lt; 首页</a></li>
                                                <li class="prev disabled"><a href="#">&lt;  上一页 </a></li>
                                                <li class="next disabled"><a href="#">下一页  &gt;</a></li>
                                                <li class="last disabled"><a href="#">末页  &gt;&gt;</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </form>
                    </div>
                </div>

            </div>

        </div>

    </div>

</div>
