@{
    ViewBag.Title = "会员积点转让";
}

<div class="row-fluid heading">
    <div class="pull-left">
        <h3>会员积点转让</h3>
    </div>
    <div class="pull-right">
        <button class="btn" onclick="add()">新建会员积点转让</button>&nbsp;
    </div>
</div>

<div class="form-inline  well">
    <label>姓名</label>
    <input type="text" id="txtName" class="input-medium">
    <label>手机号</label>
    <input type="text" id="txtMobile" class="input-medium">
    <label>开始时间</label>
    <input type="text" id="txtStart" class="input-medium">
    <label>结束时间</label>
    <input type="text" id="txtEnd" class="input-medium">
    <button id="btnSearch" class="btn">查询</button>
</div>
<div class="row-fluid">
    <div class="mt10 mbox">
        <table class="table" id="dt_transfers"></table>
    </div>
</div>

@section Scripts{
    @System.Web.Optimization.Scripts.Render("~/Scripts/Pages/CreditTransfer/Index.js")
}