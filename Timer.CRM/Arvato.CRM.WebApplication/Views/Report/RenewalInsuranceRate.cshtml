@{
    ViewBag.Title = "RenewalInsuranceRate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
   

<div class="row-fluid heading">
    <div class="pull-left">
        <h3>续保率</h3>
    </div>
</div>

<div class="sepH_c row-fluid">
    <div class="well firstShow ">
        <div class="row-fluid">
            <div class="form-inline">
                <label>手机号</label>
                <input type="text" id="txtMobile" class="span2"/>
                <label>门店名称</label>
                <input type="text" id="txtStore" class="span2" />
                <label>续保年份</label>
                <input type="text" id="txtYear" class="span2" value="@DateTime.Now.ToString("yyyy-MM-dd")"/>
                <button id="btnSerach" class="btn sepV_a">查询</button>
                <button id="export" class="btn sepV_a">导出</button>
            </div>
        </div>
    </div>
    <table class="table" data-provides="rowlink" id="dt_RenewalInsuranceRateTable">
    </table>
</div>

<form id="exportForm" method="post">    
    <input type="hidden" id="exportYear" name="exportYear" />
    <input type="hidden" id="exportStore" name="exportStore" />
    <input type="hidden" id="exportMobile" name="exportMobile" />
</form>

@section Scripts{
    @System.Web.Optimization.Scripts.Render("~/Gebo/lib/validation/jquery.validate.js", "~/Scripts/Framework/jquery.validate.extend.js")
    @System.Web.Optimization.Scripts.Render("~/Scripts/Pages/Report/RenewalInsuranceRate.js")
}


