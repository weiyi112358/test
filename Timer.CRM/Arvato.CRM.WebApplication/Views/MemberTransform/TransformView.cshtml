@{
    ViewBag.Title = "Customize";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@System.Web.Optimization.Scripts.Render("~/Gebo/lib/plupload/js/plupload.js", "~/Gebo/lib/plupload/js/plupload.gears.js", "~/Gebo/lib/plupload/js/plupload.silverlight.js", "~/Gebo/lib/plupload/js/plupload.flash.js", "~/Gebo/lib/plupload/js/plupload.browserplus.js", "~/Gebo/lib/plupload/js/plupload.html4.js", "~/Gebo/lib/plupload/js/plupload.html5.js")
<div class="row-fluid heading">

    <div class="pull-left">
        <h3>会员批量转移</h3>
    </div>
    <div class="pull-right">
        <button class="btn" id="btnTransform">批量转移</button>
    </div>
</div>

<div class="sepH_c row-fluid">

    <div class="well firstShow ">
        <div class="row-fluid" id="queryCondition">
            <div class="span3">
                <label class="title-left sepV_a ml10">分公司</label>
                <select id="searchCompany" name="searchCompany" class="span12"></select>
            </div>
            <div class="span3">
                <label class="title-left sepV_a ml10">门店</label>
                <select id="searchStore" name="searchStore" class="span12"></select>
            </div>
        </div>
        <div class="q_btn">
            <button class="btn btn-success" id="search">查询</button>
            <button class="btn" id="btnImport">导入</button>
        </div>
    </div>
    <input type="hidden" id="ckVal" />
    <table class="table" data-provides="rowlink" id="MemTable">
    </table>
    <input type="button" id="ckALL" class="btn" value="全选" />
    <input type="button" id="uckALL" class="btn" value="全不选" />
    <input type="button" id="AckALL" class="btn" value="全都选" />
</div>

<!--新建弹窗页-->
<div class="hide">
    <div id="addBrand_dialog" class="cbox_content" style="width: 200px; height: 400px">
        <div class="row-fluid heading">
            <div class="pull-left">
                <h3>批量转移</h3>
            </div>
        </div>
        <div class="formSep">
            <div class="row-fluid sepH_b" id="modalAdd">
                <div class="span3">
                    <label>分公司<span style="color: red"> * </span></label>
                    <select id="newCompany" name="newCompany" class="span12"></select>
                    <span class="error-block"></span>
                </div>
                <div class="span3">
                    <label>门店<span style="color: red"> * </span></label>
                    <select id="newStore" name="newStore" class="span12"></select>
                    <span class="error-block"></span>
                </div>

                <div class="pull-right oneLine textright">
                    <button id="btnSave" class="btn btn-primary">保存</button>
                    <button id="btnCancel" class="btn" onclick="$.colorbox.close();">取消</button>
                </div>
            </div>

        </div>
    </div>

</div>
<div class="hide">
    <div id="import_data" class="cbox_content popConfig" style="width: 800px; height: 800px">
        <div class="row-fluid heading">
            <div class="pull-left">
                <h3>文件导入</h3>
            </div>
        </div>
        <div class="formSep">
            <div class="row-fluid sepH_b" id="modalImport">
                <form class="form_validation_import">
                    <div class="span3">
                        <label>分公司<span style="color: red"> * </span></label>
                        <select id="newCompany2" name="newCompany2" class="span12"></select>
                        <span class="error-block"></span>
                    </div>
                    <div class="span3">
                        <label>门店<span style="color: red"> * </span></label>
                        <select id="newStore2" name="newStore2" class="span12"></select>
                        <span class="error-block"></span>
                    </div>
                    <div class="row-fluid">
                        <div class="span8">
                            <label>文件路径 <span class="f_req">*</span></label>
                            <div class="span1" id="container">
                                <button type="button" class="btn btn-info" id="pickfiles">选择文件</button>
                            </div>
                            <input type="text" id="tbFilePath" readonly="true" name="tbFilePath" class="span7" />

                        </div>

                    </div>
                    <div>
                        <span id="importError" class="error"></span>
                    </div>
                </form>
                <div class="pull-right oneLine textright">
                    <button class="btn" id="btnDownTemplate">下载模板</button>
                    <button class="btn btn-gebo confirm_yes" id="btnSaveImport">保存</button>
                    <button class="btn confirm_no" id="btnCancelImport" type="button" onclick="$.colorbox.close()">取消</button>

                </div>
                <div>
                </div>


            </div>
        </div>
    </div>
</div>
<div id="processingdiv" style="display: none;">
    <div class="popup">
        <div class="popup-body">
            <div class="loading" style="text-align: center; vertical-align: middle">
                <br />
                <img src="~/new_img/loading.gif" /><br />
                <span id="txtspan"></span>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    @System.Web.Optimization.Scripts.Render("~/Gebo/lib/validation/jquery.validate.js", "~/Scripts/Framework/jquery.validate.extend.js")

    @System.Web.Optimization.Scripts.Render("~/Scripts/Pages/MemberTransform/TransformView.js")
}