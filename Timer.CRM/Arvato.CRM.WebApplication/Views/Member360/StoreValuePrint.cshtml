@{
    ViewBag.Title = "储值历史";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row-fluid heading">
    <div class="pull-left">
        <h3>储值历史</h3>
    </div>
    <div class="pull-right">
        <button class="btn" onclick="window.location = '/Member360/StoreValue'">储值管理</button>&nbsp;
    </div>
</div>

<input type="hidden" id="hdnStoreName" value="@ViewBag.StoreName"/>
<input type="hidden" id="hdnPrinter" value="@ViewBag.Printer"/>
<div class="well">
    <div class="row-fluid">
        <div class="span2">
            <label>姓名</label>
            <input type="text" class="span12" id="txtName"  />
            <span class="error-block"></span>
        </div>
        <div class="span2">
            <label>手机号</label>
            <input type="text" class="span12" id="txtMobile"  />
        </div>
        <div class="span2">
            <label>车牌号</label>
            <input type="text" class="span12" id="txtVehicle"  />
            <span class="error-block"></span>
        </div>
        <div class="span2">
            <label>起始日期</label>
            <input type="text" class="span12" id="txtStartDate"  />
        </div>
        <div class="span2">
            <label>结束日期</label>
            <input type="text" class="span12" id="txtEndDate"  />
        </div>
        <div class="span2">
            <label>&nbsp;</label>
            <button type="button" id="btnSearch" class="btn">查询</button>
        </div>
    </div>
</div>

<div class="row-fluid">
    <table class="table" data-provides="rowlink" id="dt_ActHistoryData">
    </table>
</div>

@section Scripts{
    @System.Web.Optimization.Scripts.Render("~/Gebo/lib/validation/jquery.validate.js", "~/Scripts/Framework/jquery.validate.extend.js")
    @System.Web.Optimization.Scripts.Render("~/Scripts/Pages/Member360/StoreValuePrint.js")
}