@{
    ViewBag.Title = "会员360视图";
}
@section css{
    @System.Web.Optimization.Styles.Render("~/VGebo/chosen.css")
    @System.Web.Optimization.Styles.Render("~/IXCSS/Common.css")
}

<div class="row-fluid heading">
    <div class="pull-left">
        <h3>会员360视图</h3>
    </div>
</div>
<div class="form-inline well">
    <label>手机号</label>
    <input type="text" class="span2" id="txtMobile" value="" />
        &nbsp; &nbsp; &nbsp;
    <button class="btn" id="btnSearch" type="button">查询</button>
</div>
<div class="row-fluid">
    <table class="table" data-provides="rowlink" id="dtMemberList">
        <thead></thead>
        <tbody></tbody>
    </table>
</div>


@section Scripts{
    @System.Web.Optimization.Scripts.Render("~/Gebo/lib/chosen/chosen.jquery.min.js")
    @System.Web.Optimization.Scripts.Render("~/IXJS/libs/jquery.ba-resize.js")
    @System.Web.Optimization.Scripts.Render("~/Scripts/Pages/Member360/MemberForStore.js")
}
