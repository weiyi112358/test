<div class="tab-pane row-fluid" id="tabTradeHistory">
    <div class="well moreHid firstShow">
        <div class="row-fluid">
            <div class="span2">
                <label>单号</label>
                <input type="text" class="span12" id="txtTradeCode_TH" />
            </div>
            <div class="span2">
                <label>订单开始时间</label>
                <input type="text" class="datepicker span12" id="txtStart_TH" />
            </div>
            <div class="span2">
                <label>订单结束时间</label>
                <input type="text" class="datepicker span12" id="txtEnd_TH" />
            </div>
            <div class="span2">
                <label>消费门店</label>
                <select data-placeholder="全部" class="chzn_a span12" name="drpStore_TH" id="drpStore_TH">
                    <option value="">全部</option>
                    @foreach (var store in ViewBag.Stores)
                    { 
                        <option value="@store.StoreCode">@store.StoreName</option>
                    }
                </select>
            </div>
            <div class="row-fluid">
                <div class="span2">
                    <label>&nbsp;</label>
                    <button class="btn" id="btnSearchTradeHistory">查询</button>
                    @*<button class="btn">导出</button>*@
                </div>
            </div>
        </div>
    </div>
