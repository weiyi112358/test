@{
    ViewBag.Title = "AbnormalTrade";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row-fluid heading">
    <div class="pull-left">
        <h3>异常购物列表</h3>    
    </div>
    <div class="pull-right">
    <button id="btndelete" class="btn btn-danger">删除</button>
     
        @using (Html.BeginForm("AbnormalTradeToExcel", "Member360", FormMethod.Post, new {@style="span4" }))
        {
               <button class="btn btn-success">导出</button>
        }
    </div>
</div>
<div class="form-inline well">
    <label>输入名字查询</label>
    <input type="text" class="span2" id="txtTradeID" value="" />
        &nbsp; &nbsp; &nbsp;
    <button class="btn" id="btnSearch" type="button">查询</button>
</div>
<div class="row-fluid">
    <table class="table" data-provides="rowlink" id="abnormalTradeTable">     
    </table>
</div>


@section Scripts{
  @System.Web.Optimization.Scripts.Render("~/Gebo/lib/chosen/chosen.jquery.min.js")
  @System.Web.Optimization.Scripts.Render("~/Gebo/lib/validation/jquery.validate.js", "~/Scripts/Framework/jquery.validate.extend.js")
  @System.Web.Optimization.Scripts.Render("~/Gebo/lib/plupload/js/plupload.js", "~/Gebo/lib/plupload/js/plupload.gears.js", "~/Gebo/lib/plupload/js/plupload.silverlight.js", "~/Gebo/lib/plupload/js/plupload.flash.js", "~/Gebo/lib/plupload/js/plupload.browserplus.js", "~/Gebo/lib/plupload/js/plupload.html4.js", "~/Gebo/lib/plupload/js/plupload.html5.js")
  @System.Web.Optimization.Scripts.Render("~/Scripts/Pages/Member360/AbnormalTrade.js")
}
